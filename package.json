{
  "name": "mascot-survey",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:html": "vitest run --coverage --reporter html",
    "test:coverage:lcov": "vitest run --coverage --reporter lcov",
    "test:coverage:json": "vitest run --coverage --reporter json",
    "format": "pnpm format:prettier && pnpm format:standard",
    "format:prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md}'",
    "format:standard": "standard --fix",
    "lint": "standard --check"
  },
  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@fingerprintjs/fingerprintjs": "^4.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-google-recaptcha": "^3.1.0",
    "vite-plugin-env-compatible": "^2.0.1"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/vite": "^4.1.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.0",
    "prettier-config-standard": "^7.0.0",
    "standard": "^17.1.2",
    "tailwindcss": "~4.1.10",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "prettier": "prettier-config-standard"
}
